{{ header }}
<main>
	<section class="section__underlined">
	  <div class="breadcrumbs__container">
		<ul class="breadcrumbs">
		{% for breadcrumb in breadcrumbs %}
        <li class="breadcrumbs__item">
          <a class="breadcrumbs__link" href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a>
        </li>
        {% endfor %}
		</ul>
	  </div>
	</section>

	<section class="section__regular tab">
	  <div class="tab__container">
		<div class="tab__header">
		  <button class="tab__button active" data-tab="tab1">{{ tab_description }}</button>
		  <button class="tab__button" data-tab="tab2">{{ tab_attribute }}</button>
		  <button class="tab__button" data-tab="tab3">{{ tab_review }}</button>
		  <button class="tab__button" data-tab="tab4">{{ tab_video }}</button>
		</div>
		<div class="tab__wrapper tab_1__wrapper active" id="tab1">
		  <div class="tab__block tab_1__block">
			<div class="tab__content">
			  <div class="product_overview">
				<div class="product_overview__image_list">
				{% if images or kit_part %}
	               <a class="thumbnail" href="{{ popup }}" data-fancybox="images" data-caption="{{ heading_title }}" data-thumbnail="{{ thumb }}" title="{{ heading_title }}"><img src="{{ popup }}" class="product_overview__item product_overview__item--active" /></a>
				  {% for image2 in first_images %}
				  {% if (image2['video_url']) %}
				  <a class="thumbnail thumbnail_video" style="position: relative;" href="{{ image2['video_url'] }}" data-type="iframe" data-fancybox="images" data-caption="{{ heading_title }}" title="{{ heading_title }}"><div class="product_overview__item"><div class="play_button" bis_skin_checked="1"></div></div></a>
				  {% elseif image2.sort_order > 1000 %}
				  <a class="thumbnail rotate-cover mfp-iframe" href="#" data-type="iframe" data-fancybox="images" data-caption="{{ heading_title }}" data-rotate="/{{ model2 }}.html" data-thumbnail="{{ image2.thumb }}" title="{{ heading_title }}"><img class="product_overview__item" src="{{ image2.thumb }}" alt="360" /></a>
				  {% else %}
				  <a class="thumbnail" href="{{ image2.popup }}" data-fancybox="images" data-caption="{{ heading_title }}" data-thumbnail="{{ image2.thumb }}" title="{{ heading_title }}"><img src="{{ image2.thumb }}" alt="{{ heading_title }}" class="product_overview__item" /></a>
				  {% endif %}
				  {% endfor %}
				<div id="hidden-images">
				{% for image2 in last_images %}
				{% if (image2['video_url']) %}
				<div class="product_overview__item"><div class="play_button" bis_skin_checked="1"></div></div>
				{% elseif image2.sort_order > 1000 %}
				<img class="product_overview__item" src="{{ image2.thumb }}" alt="360" />
				{% else %}
				<img src="{{ image2.thumb }}" alt="{{ heading_title }}" class="product_overview__item" />
				{% endif %}
				{% endfor %}
			    </div>
				{% endif %}
				</div>
				<div class="product_overview__buttons">
				  <span class="product_overview__quantity">+{{ count_last_images }}</span>
				  <span class="product_overview__arrow"></span>
				</div>
				<div class="product_overview__image--main">
			    <a class="thumbnail" href="{{ popup }}" data-fancybox="images" data-caption="{{ heading_title }}" data-thumbnail="{{ thumb }}" title="{{ heading_title }}"><img src="{{ popup }}" alt="Product" class="product_overview__image" /></a>
				  <div class="product__labels">
				  {% if product.product_xdstickers is not empty %}
				  {% for xdsticker in product.product_xdstickers %}
					<span class="product__labels-label">{{ xdsticker.text }}</span>
				  {% endfor %}
				  {% endif %}
				  {% if product.product_xdstickers_custom is not empty %}
				  {% for xdsticker in product.product_xdstickers_custom %}
					<span class="product__labels-label">{{ xdsticker.text }}</span>
				  {% endfor %}
				  {% endif %}
				  </div>
				</div>
				<div class="product_overview__pagination">
				  {% for key, image2 in images %}
				  <span class="product_overview__pagination_item {% if key == 0 %}product_overview__pagination_item--active{% endif %}" id="pic-{{ key }}" data-image="{{ image2.popup }}" onclick="setImage({{ key }})"></span>
				 {% endfor %}
				</div>
			  </div>
			  <div class="tab_1__parameters">
				<h2 class="tab_1__parameters_title">{{ text_main_attrs }}</h2>
				<div class="product-parameters">
				  <ul class="parameters__wrapper">
				  {% for attr in main_attributes %}
					<li class="parameters__item">
					  <p class="parameters__label">{{ attr.name }}</p>
					  <p class="parameters__value">{{ attr.text }}</p>
					</li>
				  {% endfor %}
				  </ul>
				  <ul class="parameters__wrapper" id="hidden-attrs">
				  {% for attr in another_attributes %}
					<li class="parameters__item">
					  <p class="parameters__label">{{ attr.name }}</p>
					  <p class="parameters__value">{{ attr.text }}</p>
					</li>
				  {% endfor %}
				  </ul>
				</div>
			  </div>
			</div>
		  </div>

		  <aside class="tab_1__aside">
			<div class="tab_1__aside_header">
			  <div class="product__badges product__badges_page--state" bis_skin_checked="1">
			    {% if stock_pos == false %}<span class="product__badge-stock">{{ text_instock }}</span>{% endif %}
				<span class="product__badge-deco"></span>
			    {% if gifts %}<span class="product__badge-present">{{ text_gift }}</span>{% endif %}
			    {% if is_new %}<span class="product__badge-new">new</span>{% endif %}
			    {% if special %}<span class="product__badge-discount">-{{ percent }}%</span>{% endif %}
			  </div>
			  <div class="tab_1__aside_article">
				<p>
				  {{ text_model }}
				  <span>{{ model }}</span>
				</p>
			  </div>
			</div>

			<h2 class="tab_1__name">
			  <a href="#">{{ heading_title }}</a>
			</h2>
			<div class="product__rating product__rating--sigle_mod">
			  <div class="rating">
			    {% for i in 1..5 %}
				{% if product.rating >= i %}
				<span class="rating-star rating-filled"></span>
				{% else %}
				<span class="rating-star"></span>
			    {% endif %}
		        {% endfor %}
			  </div>
			  <span class="product__rating-total">{{ tab_review }}</span>
			</div>

			<div class="product_secondary product_secondary--single_mod">
			  <div class="product_secondary__wrapper">
				<div class="product_secondary__main">
				  <div class="product_secondary__content">
					<div class="product__price-row">
					  <p class="product__price">
					    {% if special %}  
						<span>{{ special }}</span>
						{% else %}
					    {{ price }}
						{% endif %}
					  </p>
					  {% if special %}  
					  <p class="product__price-old">{{ price }}</p>
					  {% endif %}
					</div>
				  </div>
				</div>
				<div class="product_secondary__buttons">
				  <button class="button" onclick="cart.add('{{ product_id }}');">
					<p>{{ button_cart }}</p>
				  </button>

				  <button type="button" class="button button_icon product_secondary__button_big">
					<span class="button_icon__icon product_secondary__button_big_icon"></span>
					{{ text_pay_part }}
				  </button>

				  <a class="button button_gray product_secondary__button_gray" {% if not wl_class %}onclick="wishlist.add({{ product_id }}, '', '{{ to_wishlist }}', '{{ from_wishlist }}')"{% else %}onclick="wishlist.remove({{ product_id }}, '', '{{ from_wishlist }}', '{{ to_wishlist }}')"{% endif %}>
					<svg class="product_secondary__button_gray_icon product-like-{{ product_id }} {% if wl_class %}product__like-active{% endif %}" viewBox="0 0 24 21" fill="none" xmlns="http://www.w3.org/2000/svg">
					  <path
						d="M11.9626 19.5381L2.10281 9.80024C1.07574 8.78576 0.5 7.41137 0.5 5.97966C0.5 4.548 1.07574 3.17365 2.10281 2.15928C3.13034 1.1449 4.52508 0.573975 5.98067 0.573975C7.43626 0.573975 8.83108 1.1449 9.85873 2.15928L11.9626 4.23506L14.0665 2.15928C15.094 1.1449 16.4888 0.573975 17.9444 0.573975C19.4 0.573975 20.7948 1.1449 21.8224 2.15928C22.8499 3.17365 23.4287 4.548 23.4287 5.97966C23.4287 7.41137 22.8499 8.78576 21.8224 9.80024L11.9626 19.5381Z"
					  />
					</svg>
					{% if not wl_class %}
				        <p class="in_wishlist">{{ text_wl }}</p>
						{% else %}
						<p class="in_wishlist">{{ text_from_wl }}</p>
						{% endif %}
				  </a>
				</div>
			  </div>
			</div>

			<div class="tab_1__aside_info">
			  <div class="info__block">
				<div class="tab_1__parameters tab_1__parameters--mob_mod">
			    <h2 class="tab_1__parameters_title">{{ text_main_attrs }}</h2>
			    <div class="product-parameters">
			      <ul class="parameters__wrapper">
			      {% for attr in main_attributes %}
				    <li class="parameters__item">
				      <p class="parameters__label">{{ attr.name }}</p>
				      <p class="parameters__value">{{ attr.text }}</p>
				    </li>
			      {% endfor %}
			      </ul>
			      <ul class="parameters__wrapper" id="hidden-attrs">
			      {% for attr in another_attributes %}
				    <li class="parameters__item">
				      <p class="parameters__label">{{ attr.name }}</p>
				      <p class="parameters__value">{{ attr.text }}</p>
				    </li>
			      {% endfor %}
			      </ul>
				  <ul class="parameters__wrapper" id="hidden-attrs2">
				  {% for attr in another_attributes %}
					<li class="parameters__item">
					  <p class="parameters__label">{{ attr.name }}</p>
					  <p class="parameters__value">{{ attr.text }}</p>
					</li>
				  {% endfor %}
				  </ul>
			    </div>
				</div>
				<div class="info">
				  <div class="info__item">
					<ul class="info__list">
					  <li class="info__list_title">Оплата</li>
					  <li class="info__list_item info__list_item--pay">{{ text_buy_1 }}</li>
					  <li class="info__list_item info__list_item--pay">{{ text_buy_2 }}</li>
					  <li class="info__list_item info__list_item--pay">{{ text_buy_3 }}</li>
					  <li class="info__list_item info__list_item--pay">{{ text_buy_4 }}</li>
					  <div class="info__list_pay_row">
						<img src="image/info/icon_01.svg" alt="" />
						<img src="image/info/icon_02.svg" alt="" />
						<img src="image/info/icon_03.svg" alt="" />
						<img src="image/info/icon_04.svg" alt="" />
						<img src="image/info/icon_05.svg" alt="" />
					  </div>
					</ul>
				  </div>
				  <div class="info__item">
					<ul class="info__list">
					  <li class="info__list_title info__list_title--delivery">{{ title_delivery }}</li>
					  <li class="info__list_item info__list_item--delivery">
						{{ text_delivery_1 }}
					  </li>
					<li class="info__list_item info__list_item--delivery">{{ text_delivery_2 }}</li>
					</ul>
				  </div>
				</div>
				<div class="info">
				  <div class="info__item">
					<ul class="info__list">
					  <li class="info__list_item info__list_item--guarantee">
						<strong>{{ title_garanty }}</strong>
						{{ garanties }}
					  </li>
					</ul>
				  </div>
				</div>
			  </div>
			</div>
            
			{% if product_gifts %}
			<div class="tab_1__present">
			  <div class="tab_1__present_title">
				<h3>
				  {{ text_gift }}
				  <span>×{{ count_gifts }}</span>
				</h3>
			  </div>
			  <div class="tab_1__present_wrapper">
			  {% for gift in product_gifts %}
				<div class="product_present">
				  <span class="product_present__number">{{ gift.i }}</span>
				  <div class="product_present__image_wraper">
					<img class="product_present__image" src="{{ gift.thumb }}" alt="Present image" />
				  </div>
				  <div class="product_present__content">
					<h4 class="product_present__title">{{ gift.name }}</h4>
					<div class="product_present__options">
					  <span class="product_present__price">{{ gift.last_price }}</span>
					</div>
				  </div>
				</div>
			  {% endfor %}
			  </div>
			</div>
		    {% endif %}
		  </aside>
		</div>
		<div class="tab__wrapper" id="tab2">
            <div class="tab__block">
              <div class="tab__content">
                <div class="tab_2__body">
                  <h2 class="tab_2__title">{{ tab_attribute }}</h2>
                  <div class="tab_in__header">
                    <button class="tab_in__button active" data-tab="tabIn1">{{ text_tech_attrs }}</button>
                    <button class="tab_in__button" data-tab="tabIn2">{{ tab_attribute_parts }}</button>
                  </div>
                  <div class="tab_in__content" id="tabIn1">
