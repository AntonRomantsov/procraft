{% if page_form_options %}
  {% for page_form_option in page_form_options %}
    {% if page_form_option.type == 'select' %}
      <div class="form-group{{ page_form_option.required ? ' required' : '' }} {{ page_form_option.width ? 'col-sm-'~ page_form_option.width : '' }} {{ page_form_option.class ? page_form_option.class : '' }}">
        <label class="control-label" for="input-field{{ page_form_option.page_form_option_id }}">{{ page_form_option.field_name }}
          {% if page_form_option.field_help %}
            <i class="fa fa-question-circle" data-toggle="tooltip" title="{{ page_form_option.field_help }}"></i>
          {% endif %}
        </label>
        <select name="field[{{ page_form_option.page_form_option_id }}]" id="input-field{{ page_form_option.page_form_option_id }}" class="form-control">
          <option value="">{{ text_select }}</option>
          {% for page_form_option_value in page_form_option.page_form_option_value %}
          {% if page_form_option_value.default_value %}
            {% set sel = 'selected="selected"' %}
          {% else %}
            {% set sel = '' %}
          {% endif %}
          <option value="{{ page_form_option_value.page_form_option_value_id }}" {{ sel }}>{{ page_form_option_value.name }}</option>
          {% endfor %}
        </select>
      </div>
      {% endif %}
      {% if page_form_option.type == 'radio' %}
      <div class="form-group{{ page_form_option.required ? ' required' : '' }} {{ page_form_option.width ? 'col-sm-'~ page_form_option.width : '' }} {{ page_form_option.class ? page_form_option.class : '' }}">
        <label class="control-label">{{ page_form_option.field_name }}
          {% if page_form_option.field_help %}
            <i class="fa fa-question-circle" data-toggle="tooltip" title="{{ page_form_option.field_help }}"></i>
          {% endif %}
        </label>
        <div id="input-field{{ page_form_option.page_form_option_id }}">
          {% for page_form_option_value in page_form_option.page_form_option_value %}
          <div class="radio-inline">
            <label>
              {% if page_form_option_value.default_value %}
                {% set sel = 'selected="selected"' %}
              {% else %}
                {% set sel = '' %}
              {% endif %}
              <input type="radio" name="field[{{ page_form_option.page_form_option_id }}]" value="{{ page_form_option_value.page_form_option_value_id }}" {{ sel }} />
              {{ page_form_option_value.name }}
            </label>
          </div>
          {% endfor %}
        </div>
      </div>
      {% endif %}
      {% if page_form_option.type == 'checkbox' %}
      <div class="form-group{{ page_form_option.required ? ' required' : '' }} {{ page_form_option.width ? 'col-sm-'~ page_form_option.width : '' }} {{ page_form_option.class ? page_form_option.class : '' }}">
        <label class="control-label">{{ page_form_option.field_name }}
          {% if page_form_option.field_help %}
            <i class="fa fa-question-circle" data-toggle="tooltip" title="{{ page_form_option.field_help }}"></i>
          {% endif %}
        </label>
        <div id="input-field{{ page_form_option.page_form_option_id }}">
          {% for page_form_option_value in page_form_option.page_form_option_value %}
          <div class="checkbox">
            <label>
              {% if page_form_option_value.default_value %}
                {% set sel = 'selected="selected"' %}
              {% else %}
                {% set sel = '' %}
              {% endif %}
              <input type="checkbox" name="field[{{ page_form_option.page_form_option_id }}][]" value="{{ page_form_option_value.page_form_option_value_id }}" {{ sel }} />
              {{ page_form_option_value.name }}
            </label>
          </div>
          {% endfor %}
        </div>
      </div>
      {% endif %}

      {% if page_form_option.type == 'checkbox_toggle' %}
      <div class="form-group{{ page_form_option.required ? ' required' : '' }} {{ page_form_option.width ? 'col-sm-'~ page_form_option.width : '' }} {{ page_form_option.class ? page_form_option.class : '' }}">
        <label class="control-label">{{ page_form_option.field_name }}
          {% if page_form_option.field_help %}
            <i class="fa fa-question-circle" data-toggle="tooltip" title="{{ page_form_option.field_help }}"></i>
          {% endif %}
        </label>
        <div id="input-field{{ page_form_option.page_form_option_id }}">
          {% for page_form_option_value in page_form_option.page_form_option_value %}
          <div class="checkbox">
            <label class="ci-switch">
              {% if page_form_option_value.default_value %}
                {% set sel = 'selected="selected"' %}
                {% set sel_active = 'active' %}
              {% else %}
                {% set sel = '' %}
                {% set sel_active = '' %}
              {% endif %}
              <input type="checkbox" name="field[{{ page_form_option.page_form_option_id }}][]" value="{{ page_form_option_value.page_form_option_value_id }}" {{ sel }} />
              <span class="ci-slider round"></span>
            </label>
            <label>
            {{ page_form_option_value.name }}
            </label>
          </div>
          {% endfor %}
        </div>
      </div>
      {% endif %}

      {% if page_form_option.type == 'multi_select' %}
      <div class="form-group{{ page_form_option.required ? ' required' : '' }} {{ page_form_option.width ? 'col-sm-'~ page_form_option.width : '' }} {{ page_form_option.class ? page_form_option.class : '' }}">
        <label class="control-label">{{ page_form_option.field_name }}
          {% if page_form_option.field_help %}
            <i class="fa fa-question-circle" data-toggle="tooltip" title="{{ page_form_option.field_help }}"></i>
          {% endif %}
        </label>
        <select multiple name="field[{{ page_form_option.page_form_option_id }}][]" id="input-field{{ page_form_option.page_form_option_id }}" class="form-control multiselect">
          {% for page_form_option_value in page_form_option.page_form_option_value %}
          {% if page_form_option_value.default_value %}
            {% set sel = 'selected="selected"' %}
          {% else %}
            {% set sel = '' %}
          {% endif %}
          <option value="{{ page_form_option_value.page_form_option_value_id }}" {{ sel }}>{{ page_form_option_value.name }}
          </option>
          {% endfor %}
        </select>
      </div>
      {% endif %}

      {% if page_form_option.type == 'radio_toggle' %}
      <div class="form-group{{ page_form_option.required ? ' required' : '' }} {{ page_form_option.width ? 'col-sm-'~ page_form_option.width : '' }} {{ page_form_option.class ? page_form_option.class : '' }}">
        <label class="control-label">{{ page_form_option.field_name }}
          {% if page_form_option.field_help %}
            <i class="fa fa-question-circle" data-toggle="tooltip" title="{{ page_form_option.field_help }}"></i>
          {% endif %}
        </label>
        <div id="input-field{{ page_form_option.page_form_option_id }}" class="btn-group" data-toggle="buttons">
          {% set sel_count = 0 %}
          {% for page_form_option_value in page_form_option.page_form_option_value %}
            {% if sel_count == 0 and page_form_option_value.default_value %}
              {% set sel = 'checked="checked"' %}
              {% set sel_active = 'active' %}
              {% set sel_count = sel_count + 1 %}
            {% else %}
              {% set sel = '' %}
              {% set sel_active = '' %}
            {% endif %}
            <label class="btn btn-default {{ sel_active }}">
              <input name="field[{{ page_form_option.page_form_option_id }}]"  autocomplete="off" value="{{ page_form_option_value.page_form_option_value_id }}" type="radio" {{ sel }}>{{ page_form_option_value.name }}
            </label>
          {% endfor %}
        </div>
      </div>
      {% endif %}

      {% if page_form_option.type in ['text', 'number', 'telephone', 'email', 'email_exists', 'postcode', 'address'] %}
      <div class="form-group{{ page_form_option.required ? ' required' : '' }} {{ page_form_option.width ? 'col-sm-'~ page_form_option.width : '' }} {{ page_form_option.class ? page_form_option.class : '' }}">
        <label class="control-label" for="input-field{{ page_form_option.page_form_option_id }}">{{ page_form_option.field_name }}
          {% if page_form_option.field_help %}
            <i class="fa fa-question-circle" data-toggle="tooltip" title="{{ page_form_option.field_help }}"></i>
          {% endif %}
        </label>
        <input type="text" name="field[{{ page_form_option.page_form_option_id }}]" id="input-field{{ page_form_option.page_form_option_id }}" value="{{ page_form_option.field_dvalue }}" class="form-control" placeholder="{{ page_form_option.field_placeholder }}">
      </div>
    {% endif %}
    {% if page_form_option.type == 'textarea' %}
      <div class="form-group{{ page_form_option.required ? ' required' : '' }} {{ page_form_option.width ? 'col-sm-'~ page_form_option.width : '' }} {{ page_form_option.class ? page_form_option.class : '' }}">
        <label class="control-label" for="input-field{{ page_form_option.page_form_option_id }}">{{ page_form_option.field_name }}
          {% if page_form_option.field_help %}
            <i class="fa fa-question-circle" data-toggle="tooltip" title="{{ page_form_option.field_help }}"></i>
          {% endif %}
        </label>
        <textarea name="field[{{ page_form_option.page_form_option_id }}]" rows="5" placeholder="{{ page_form_option.field_placeholder }}" id="input-field{{ page_form_option.page_form_option_id }}" class="form-control">{{ page_form_option.field_dvalue }}</textarea>
      </div>
      {% endif %}

      {% if page_form_option.type == 'color_picker' %}
      <div class="form-group{{ page_form_option.required ? ' required' : '' }} {{ page_form_option.width ? 'col-sm-'~ page_form_option.width : '' }} {{ page_form_option.class ? page_form_option.class : '' }}">
        <label class="control-label">{{ page_form_option.field_name }}
          {% if page_form_option.field_help %}
            <i class="fa fa-question-circle" data-toggle="tooltip" title="{{ page_form_option.field_help }}"></i>
          {% endif %}
        </label>
        <input type="text" name="field[{{ page_form_option.page_form_option_id }}]" id="input-field{{ page_form_option.page_form_option_id }}" value="{{ page_form_option.field_dvalue }}" class="form-control color-picker" placeholder="{{ page_form_option.field_placeholder }}">
      </div>
      {% endif %}

      {% if page_form_option.type == 'google_map' %}
        {% if page_form_option.field_help %}
        <div class="form-group {{ page_form_option.width ? 'col-sm-'~ page_form_option.width : '' }} {{ page_form_option.class ? page_form_option.class : '' }}">
          <iframe src="{{ page_form_option.field_help }}" width="100%" height="250" frameborder="0" style="border:0" allowfullscreen></iframe>
        </div>
        {% endif %}
      {% endif %}

      {% if page_form_option.type == 'file' %}
      <div class="form-group{{ page_form_option.required ? ' required' : '' }} {{ page_form_option.width ? 'col-sm-'~ page_form_option.width : '' }} {{ page_form_option.class ? page_form_option.class : '' }} upload-wise">
        <label class="control-label">{{ page_form_option.field_name }}
          {% if page_form_option.field_help %}
            <i class="fa fa-question-circle" data-toggle="tooltip" title="{{ page_form_option.field_help }}"></i>
          {% endif %}
        </label>
        <div class="dropzone dropzone-file-area" id="dropzone-upload{{ page_form_option.page_form_option_id }}" data-formid="{{ page_form_id }}">
          <div class="dz-default dz-message">
            <h4 class="sbold"><i class="fa fa-cloud-upload"></i></h4>
            <label for="file"><strong>Choose a file</strong><span class="box__dragndrop"> or drag it here</span>.</label>
          </div>
        </div>
        <input type="hidden" name="field[{{ page_form_option.page_form_option_id }}]" value="" id="input-field{{ page_form_option.page_form_option_id }}" />
      </div>
      {% endif %}

      {% if page_form_option.type == 'date' %}
      <div class="form-group{{ page_form_option.required ? ' required' : '' }} {{ page_form_option.width ? 'col-sm-'~ page_form_option.width : '' }} {{ page_form_option.class ? page_form_option.class : '' }}">
        <label class="control-label" for="input-field{{ page_form_option.page_form_option_id }}">{{ page_form_option.field_name }}
          {% if page_form_option.field_help %}
            <i class="fa fa-question-circle" data-toggle="tooltip" title="{{ page_form_option.field_help }}"></i>
          {% endif %}
        </label>
        <div class="input-group date">
          <input type="text" name="field[{{ page_form_option.page_form_option_id }}]" value="{{ page_form_option.field_dvalue }}" data-date-format="YYYY-MM-DD" id="input-field{{ page_form_option.page_form_option_id }}" class="form-control" placeholder="{{ page_form_option.field_placeholder }}" />
          <span class="input-group-btn">
          <button class="btn btn-default" type="button"><i class="fa fa-calendar"></i></button>
          </span>
        </div>
      </div>
      {% endif %}
      {% if page_form_option.type == 'datetime' %}
      <div class="form-group{{ page_form_option.required ? ' required' : '' }} {{ page_form_option.width ? 'col-sm-'~ page_form_option.width : '' }} {{ page_form_option.class ? page_form_option.class : '' }}">
        <label class="control-label" for="input-field{{ page_form_option.page_form_option_id }}">{{ page_form_option.field_name }}
          {% if page_form_option.field_help %}
            <i class="fa fa-question-circle" data-toggle="tooltip" title="{{ page_form_option.field_help }}"></i>
          {% endif %}
        </label>
        <div class="input-group datetime">
          <input type="text" name="field[{{ page_form_option.page_form_option_id }}]" value="{{ page_form_option.field_dvalue }}" data-date-format="YYYY-MM-DD HH:mm" id="input-field{{ page_form_option.page_form_option_id }}" class="form-control" placeholder="{{ page_form_option.field_placeholder }}" />
          <span class="input-group-btn">
          <button type="button" class="btn btn-default"><i class="fa fa-calendar"></i></button>
          </span>
        </div>
      </div>
      {% endif %}
      {% if page_form_option.type == 'time' %}
      <div class="form-group{{ page_form_option.required ? ' required' : '' }} {{ page_form_option.width ? 'col-sm-'~ page_form_option.width : '' }} {{ page_form_option.class ? page_form_option.class : '' }}">
        <label class="control-label" for="input-field{{ page_form_option.page_form_option_id }}">{{ page_form_option.field_name }}
          {% if page_form_option.field_help %}
            <i class="fa fa-question-circle" data-toggle="tooltip" title="{{ page_form_option.field_help }}"></i>
          {% endif %}
        </label>
        <div class="input-group time">
          <input type="text" name="field[{{ page_form_option.page_form_option_id }}]" value="{{ page_form_option.field_dvalue }}" data-date-format="HH:mm" id="input-field{{ page_form_option.page_form_option_id }}" class="form-control" placeholder="{{ page_form_option.field_placeholder }}" />
          <span class="input-group-btn">
          <button type="button" class="btn btn-default"><i class="fa fa-calendar"></i></button>
          </span>
        </div>
      </div>
    {% endif %}
    {% if page_form_option.type == 'country' %}
      <div class="form-group{{ page_form_option.required ? ' required' : '' }} {{ page_form_option.width ? 'col-sm-'~ page_form_option.width : '' }} {{ page_form_option.class ? page_form_option.class : '' }}">
        <label class="control-label" for="input-field{{ page_form_option.page_form_option_id }}">{{ page_form_option.field_name }}
          {% if page_form_option.field_help %}
            <i class="fa fa-question-circle" data-toggle="tooltip" title="{{ page_form_option.field_help }}"></i>
          {% endif %}
        </label>
        <select name="field[{{ page_form_option.page_form_option_id }}]" id="input-field{{ page_form_option.page_form_option_id }}" class="form-control country_id">
          <option value="">{{ text_select }}</option>
          {% for country in countries %}
          <option value="{{ country.country_id }}">{{ country.name }}
          </option>
          {% endfor %}
        </select>
      </div>
    {% endif %}
    {% if page_form_option.type == 'zone' %}
      <div class="form-group{{ page_form_option.required ? ' required' : '' }} {{ page_form_option.width ? 'col-sm-'~ page_form_option.width : '' }} {{ page_form_option.class ? page_form_option.class : '' }}">
        <label class="control-label" for="input-field{{ page_form_option.page_form_option_id }}">{{ page_form_option.field_name }}
          {% if page_form_option.field_help %}
            <i class="fa fa-question-circle" data-toggle="tooltip" title="{{ page_form_option.field_help }}"></i>
          {% endif %}
        </label>
        {% if country_exists %}
        <select name="field[{{ page_form_option.page_form_option_id }}]" id="input-field{{ page_form_option.page_form_option_id }}" class="form-control zone_id"></select>
        {% else %}
        <select name="field[{{ page_form_option.page_form_option_id }}]" id="input-field{{ page_form_option.page_form_option_id }}" class="form-control zone_id">
          <option value="">{{ text_select }}</option>
          {% for zone in zones %}
          <option value="{{ zone.zone_id }}">{{ zone.name }}
          </option>
          {% endfor %}
        </select>
        {% endif %}
      </div>
    {% endif %}
    {% if page_form_option.type in ['password', 'confirm_password'] %}
      <div class="form-group{{ page_form_option.required ? ' required' : '' }} {{ page_form_option.width ? 'col-sm-'~ page_form_option.width : '' }} {{ page_form_option.class ? page_form_option.class : '' }}">
        <label class="control-label" for="input-field{{ page_form_option.page_form_option_id }}">{{ page_form_option.field_name }}
          {% if page_form_option.field_help %}
            <i class="fa fa-question-circle" data-toggle="tooltip" title="{{ page_form_option.field_help }}"></i>
          {% endif %}
        </label>
        <input type="password" name="field[{{ page_form_option.page_form_option_id }}]" id="input-field{{ page_form_option.page_form_option_id }}" value="" class="form-control">
      </div>
    {% endif %}

    {% if page_form_option.type == 'header' %}
      <div class="col-sm-12">
        <h3 class="ci-header {{ page_form_option.class ? page_form_option.class : '' }}">{{ page_form_option.field_name }}</h3>
      </div>
    {% endif %}

    {% if page_form_option.type == 'paragraph' %}
    <div class="col-sm-12">
      <p class="ci-paragraph {{ page_form_option.class ? page_form_option.class : '' }}">{{ page_form_option.field_name }}</p>
    </div>
    {% endif %}

    {% if page_form_option.type == 'hrline' %}
      <div class="col-sm-12">
        <hr class="ci-hrline {{ page_form_option.class ? page_form_option.class : '' }}" />
      </div>
    {% endif %}

  {% endfor %}
{% endif %}