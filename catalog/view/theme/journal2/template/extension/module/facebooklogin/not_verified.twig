{{ header }}
<div id="container" class="container">
  <ul class="breadcrumb">
    {% for $breadcrumb in $breadcrumbs %}
    <li><a href="{{ breadcrumb['href'] }}">{{ breadcrumb['text'] }}</a></li>
    {% endfor %}
  </ul>
  <div class="alert alert-danger"><i class="fa fa-exclamation-circle"></i> {{ not_verified_text }}</div>
  <div class="row">
    <div id="content" class="xs-100 sm-100 md-100 lg-100 xl-100">
      <h1>{{ heading_title }}</h1>
      <p><{{ text_description }}</p>
    </div>
  </div>
</div>
{{ footer }}